<h2>My Tasks:</h2>
<table>
    <thead>
        <tr>
            <th>Pinned</th>
            <th>Progress</th>
            <th>Title</th>
            <th>Due Date</th>
            <th>Priority</th>
            <th>Tags</th>
        </tr>
    </thead>
    <tbody>
        {{#each tasks}}
        <tr>
            <td>
                {{#if this.pinned}}
                    ðŸ“Œ
                {{/if}}
            </td>
            <td>{{this.progress}}</td>
